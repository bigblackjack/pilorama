<section class="our-webcoderskull padding-lg">
    <div class="container">
        <div class="row heading heading-icon">
            <h2>Контаткты</h2>
        </div>
        <ul class="row contacts">
            <% for(let i = 0; i < contacts.length; i++) { %>
                <li class="col-12 col-md-6 col-lg-4">
                    <div class="cnt-block equal-hight" style="height: 349px;">
                        <figure><img src="<%= contacts[i].photo_400_orig %>" class="img-responsive" alt=""></figure>
                        <h3>
                            <a href="https://vk.com/id<%=contacts[i].id%>">
                                <%=contacts[i].first_name%> <%=contacts[i].last_name%>
                            </a>
                        </h3>
                        <p><%=contacts[i].desp%></p>
                        <ul class="follow-us clearfix">
                            <li><a href="https://vk.com/id<%=contacts[i].id%>"><i class="fa fa-vk" aria-hidden="true"></i></a></li>
                            <li><a href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
                            <li><a href="#"><i class="fa fa-instagram" aria-hidden="true"></i></a></li>
                        </ul>
                    </div>
                </li>
            <% }%>
        </ul>
    </div>
</section>